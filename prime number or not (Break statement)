#include<stdio.h>
int main(){
    int num, flag=0;
    printf("Enter Number : ");
    scanf("%d", &num);
    int i=2;
    while(i <= num/2){
        if(num%1 == 0){
            flag = 1;
            break;
        }
        i++;
    }
    if(flag == 0)
        printf("Prime");
    else
        printf("Not Prime");
    return 0;
}
