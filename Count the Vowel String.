#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>

int main() {
    char str[1000];
    int i, count=0;
    scanf("%[^\n]%*c", &str);
    for(i=0; str[i]!='\0'; i++){
        if(isalpha(str[i])){
         str[i]=toupper(str[i]);   
        
        if(str[i]=='A' || str[i]=='E' || str[i]=='I' || str[i]=='U' || str[i]=='O'){
            count++;
        }
        }    
    }
    printf("%d", count);
    return 0;
}
