#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>

int main() {
    char s1[1000], rstr[1000];
    fgets(s1, sizeof(s1), stdin); // gets(s1)
    // logic to convert string into lowercase
    for(int i=0; s1[i] !='0'; i++){
        s1[i]=tolower(s1[i]);
    }
    // logic to reverse a string
    int k=0;
    for(int i=strlen(s1)-1; i>=0; i--){
        rstr[k++]= s1[i];
    }
    rstr[k]='\0';
    if(strcmp(s1,rstr)==0)
        printf("Palindrome");
    else
        printf("Not Palindrome");
    return 0;
}
