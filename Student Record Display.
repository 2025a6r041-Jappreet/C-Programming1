#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char name[30], branch[30], sec[4], college[30];
    int roll;
    scanf("%d",&roll);
    scanf("\n%[^\n]%*c",&name);
    scanf("%[^\n]%*c",&branch);
    scanf("%[^\n]%*c",&sec);
    scanf("%[^\n]%*c",&college);
    printf("Roll Number: %d\n",roll);
    printf("Name: %s\n",name);
    printf("Branch: %s\n",branch);
    printf("Section: %s\n",sec);
    printf("College: %s",college);
    return 0;
}
