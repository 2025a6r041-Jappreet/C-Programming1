#include <stdio.h>
int main(){
    char ch;
    printf("Enter Character :");
    scanf("%c",&ch);
    if((ch>=65 && ch<=90)|| (ch>=90 && ch<+122))
    printf("%c is Alphabet !!!",ch);
    else if(ch>=48 && ch<=57)
    printf("%c is digit !!!",ch);
    else
    printf("%c is symbol !!!",ch);
    return 0;
}
    
