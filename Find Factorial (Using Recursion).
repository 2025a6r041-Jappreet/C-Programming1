#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int factorial(int num){
    if(num == 0)
        return 1;
    else
        return num * factorial(num-1);
}
int main(){
    int n;
    scanf("%d",&n);
    int fact = factorial(n);
    printf("The Factorial is : %d",fact);
    return 0;
}

