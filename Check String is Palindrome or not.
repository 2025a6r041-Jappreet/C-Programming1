#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>

int main() {
    char s[1000],r[1000];
    
    scanf("%[^\n]%*c",s);
    
    int len = strlen(s);
    
    for(int i=0;i<len;i++){
        s[i]= tolower(s[i]);
    }
    
    for(int i=0; i<len;i++){
        r[i]=s[len-1-i];
    }
    
    r[len]='\0';
        
    if(strcmp(s,r)==0)
        printf("Palindrome");
    
    else
        printf("Not Palindrome");
    
    return 0;
}
