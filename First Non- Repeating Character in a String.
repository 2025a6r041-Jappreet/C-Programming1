#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char string[1000];
    fgets(string, sizeof(string), stdin);
    int index=-1;
    for(int i=0; i<strlen(string); i++){
        int count=0;
        for(int j=0; j<strlen(string); j++){
            if(string[i] == string[j])
                count++;
        }
        if(count == 1){
            index=i;
            break;
        }
    }
    if(index == -1){
        printf("-1");
    }
    else
        printf("%c",string[index]);
    return 0;
}
